{
  "name": "graphane",
  "version": "0.1.0-alpha.1",
  "description": "Data Visualization Microframework",
  "author": "Graphane",
  "license": "MIT",
  "scripts": {
    "workbench:template": "node ./tools/workbench -l template -i /src/component/template.js -t test/component/template/cases -p 7000",
    "workbench:core": "node ./tools/workbench -l core -t test/core/cases -p 7001",
    "workbench:gsvg": "node ./tools/workbench -l gSVG -i /src/lib/gsvg.script.js -t test/lib/gsvg/cases -p 7002",
    "workbench:gsvg.animateto": "node ./tools/workbench -l gSVG.animateTo -i /src/lib/gsvg.script.js -i /src/lib/plugins/gsvg.animateto.script.js -t test/lib/plugins/animateto/cases -p 7003",
    "workbench:gsvg.debug": "node ./tools/workbench -l gSVG.debug -i /src/lib/gsvg.script.js -i /src/lib/plugins/gsvg.debug.script.js -t test/lib/plugins/debug/cases -p 7004",
    "workbench:gsvg.keep.aspect": "node ./tools/workbench -l gSVG.keep.aspect -i /src/lib/gsvg.script.js -i /src/lib/plugins/gsvg.keep.aspect.script.js -t test/lib/plugins/keep.aspect/cases -p 7005",
    "workbench:gsvg.observe.resize": "node ./tools/workbench -l gSVG.observe.resize -i /src/lib/gsvg.script.js -i /src/lib/plugins/gsvg.observe.resize.script.js -t test/lib/plugins/observe.resize/cases -p 7006",
    "workbench:gsvg.observe.style": "node ./tools/workbench -l gSVG.observe.style -i /src/lib/gsvg.script.js -i /src/lib/plugins/gsvg.observe.style.script.js -t test/lib/plugins/observe.style/cases -p 7007",
    "workbench:gsvg.template.engine": "node ./tools/workbench -l gSVG.template.engine -i /src/lib/gsvg.script.js -i /src/lib/plugins/gsvg.template.engine.script.js -t test/lib/plugins/template.engine/cases -p 7008",
    "workbench:gsvg.shapes": "node ./tools/workbench -l gSVG.shapes -i /src/lib/gsvg.script.js -i /src/lib/plugins/gsvg.shapes.script.js -t test/lib/plugins/shapes/cases -p 7009",
    "test": "concurrently -m 1 npm:test:*",
    "test:helpers": "playwright test ./test/helpers/ --project=helpers",
    "test:template": "playwright test test/component/template/index.test.js --project=template",
    "test:core": "playwright test test/core/index.test.js --project=core",
    "test:gsvg": "playwright test test/lib/gsvg/index.test.js --project=svg",
    "test:gsvg.animateto": "playwright test test/lib/plugins/animateto/index.test.js --project=svg.animateto",
    "test:gsvg.debug": "playwright test test/lib/plugins/debug/index.test.js --project=svg.debug",
    "test:gsvg.keep.aspect": "playwright test test/lib/plugins/keep.aspect/index.test.js --project=svg.keep.aspect",
    "test:gsvg.observe.resize": "playwright test test/lib/plugins/observe.resize/index.test.js --project=svg.observe.resize",
    "test:gsvg.observe.style": "playwright test test/lib/plugins/observe.style/index.test.js --project=svg.observe.style",
    "test:gsvg.template.engine": "playwright test test/lib/plugins/template.engine/index.test.js --project=svg.template.engine",
    "test:gsvg.shapes": "playwright test test/lib/plugins/shapes/index.test.js --project=svg.shapes",
    "build": "concurrently -m 1 npm:build:*",
    "build:template": "node ./tools/packer/index.js ./src/component/template.js ./dist/component/template.js ./package.json",
    "build:gsvg": "node ./tools/packer/index.js ./src/lib/gsvg.script.js ./dist/lib/gsvg.js ./package.json",
    "build:gsvg.animateto": "node ./tools/packer/index.js ./src/lib/plugins/gsvg.animateto.script.js ./dist/lib/plugins/gsvg.animateto.js",
    "build:gsvg.debug": "node ./tools/packer/index.js ./src/lib/plugins/gsvg.debug.script.js ./dist/lib/plugins/gsvg.debug.js",
    "build:gsvg.keep.aspect":  "node ./tools/packer/index.js ./src/lib/plugins/gsvg.keep.aspect.script.js ./dist/lib/plugins/gsvg.keep.aspect.js",
    "build:gsvg.observe.resize": "node ./tools/packer/index.js ./src/lib/plugins/gsvg.observe.resize.script.js ./dist/lib/plugins/gsvg.observe.resize.js",
    "build:gsvg.observe.style":  "node ./tools/packer/index.js ./src/lib/plugins/gsvg.observe.style.script.js ./dist/lib/plugins/gsvg.observe.style.js",
    "build:gsvg.template.engine": "node ./tools/packer/index.js ./src/lib/plugins/gsvg.template.engine.script.js ./dist/lib/plugins/gsvg.template.engine.js",
    "build:gsvg.shapes":  "node ./tools/packer/index.js ./src/lib/plugins/gsvg.shapes.script.js ./dist/lib/plugins/gsvg.shapes.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "commander": "^11.0.0",
    "concurrently": "^8.2.1",
    "open": "^9.1.0",
    "playwright": "^1.38.1",
    "ws": "^8.14.2",
    "esbuild": "^0.19.4",
    "esprima-next": "^5.8.4"
  },
  "type": "module"
}
