{
  "name": "graphane",
  "version": "0.1.0-alpha.1",
  "description": "Data Visualization Microframework",
  "author": "Graphane",
  "license": "MIT",
  "scripts": {
    "web-component:workbench": "node ./tools/workbench -l web-component -t test/web-component/cases -p 7001",
    "web-component:test": "playwright test test/web-component/index.test.js --project=web-component",
    "svg:workbench": "node ./tools/workbench -l gSVG -i /src/svg/gsvg.script.js -t test/svg/cases -p 7002",
    "svg:test": "playwright test test/svg/index.test.js --project=svg",
    "svg.animateto:workbench": "node ./tools/workbench -l gSVG.animateTo -i /src/svg/gsvg.script.js -i /src/svg/plugins/gsvg.animateto.script.js -t test/svg/plugins/animateto/cases -p 7003",
    "svg.animateto:test": "playwright test test/svg/plugins/animateto/index.test.js --project=svg.animateto",
    "lib:test": "playwright test ./test/lib/ --project=lib",
    "test": "concurrently \"npm run web-component:test\" \"npm run svg:test\" \"npm run svg.animateto:test\""
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "commander": "^11.0.0",
    "concurrently": "^8.2.1",
    "open": "^9.1.0",
    "playwright": "^1.38.1",
    "ws": "^8.14.2"
  },
  "type": "module"
}
