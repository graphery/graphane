{
  "name": "graphane",
  "version": "0.1.0-alpha.1",
  "description": "Data Visualization Microframework",
  "author": "Graphane",
  "license": "MIT",
  "scripts": {
    "workbench:web-component": "node ./tools/workbench -l web-component -t test/web-component/cases -p 7001",
    "workbench:svg": "node ./tools/workbench -l gSVG -i /src/svg/gsvg.script.js -t test/svg/cases -p 7002",
    "workbench:svg.animateto": "node ./tools/workbench -l gSVG.animateTo -i /src/svg/gsvg.script.js -i /src/svg/plugins/gsvg.animateto.script.js -t test/svg/plugins/animateto/cases -p 7003",
    "workbench:svg.debug": "node ./tools/workbench -l gSVG.debug -i /src/svg/gsvg.script.js -i /src/svg/plugins/gsvg.debug.script.js -t test/svg/plugins/debug/cases -p 7004",
    "workbench:svg.keep.aspect": "node ./tools/workbench -l gSVG.keep.aspect -i /src/svg/gsvg.script.js -i /src/svg/plugins/gsvg.keep.aspect.script.js -t test/svg/plugins/keep.aspect/cases -p 7005",
    "workbench:svg.observe.resize": "node ./tools/workbench -l gSVG.observe.resize -i /src/svg/gsvg.script.js -i /src/svg/plugins/gsvg.observe.resize.script.js -t test/svg/plugins/observe.resize/cases -p 7006",
    "workbench:svg.observe.style": "node ./tools/workbench -l gSVG.observe.style -i /src/svg/gsvg.script.js -i /src/svg/plugins/gsvg.observe.style.script.js -t test/svg/plugins/observe.style/cases -p 7007",
    "test": "concurrently -m 1 npm:test:*",
    "test:helpers": "playwright test ./test/helpers/ --project=helpers",
    "test:web-component": "playwright test test/web-component/index.test.js --project=web-component",
    "test:svg": "playwright test test/svg/index.test.js --project=svg",
    "test:svg.animateto": "playwright test test/svg/plugins/animateto/index.test.js --project=svg.animateto",
    "test:svg.debug": "playwright test test/svg/plugins/debug/index.test.js --project=svg.debug",
    "test:svg.keep.aspect": "playwright test test/svg/plugins/keep.aspect/index.test.js --project=svg.keep.aspect",
    "test:svg.observe.resize": "playwright test test/svg/plugins/observe.resize/index.test.js --project=svg.observe.resize",
    "test:svg.observe.style": "playwright test test/svg/plugins/observe.style/index.test.js --project=svg.observe.style"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "commander": "^11.0.0",
    "concurrently": "^8.2.1",
    "open": "^9.1.0",
    "playwright": "^1.38.1",
    "ws": "^8.14.2"
  },
  "type": "module"
}
