{
  "name": "graphane",
  "version": "0.1.0-alpha.4",
  "description": "Data Visualization Microframework",
  "author": "Graphane",
  "license": "MIT",
  "scripts": {
    "workbench:composer": "node ./tools/workbench -l composer -i /src/component/composer.js -t test/component/composer/cases -p 7000",
    "workbench:core": "node ./tools/workbench -l core -t test/core/cases -p 7001",
    "workbench:gsvg": "node ./tools/workbench -l gSVG -i /src/lib/gsvg.script.js -t test/lib/gsvg/cases -p 7002",
    "workbench:plugin.animateto": "node ./tools/workbench -l plugin.animateTo -i /src/lib/gsvg.script.js -i /src/plugins/animateto.script.js -t test/plugins/animateto/cases -p 7003",
    "workbench:plugin.debug": "node ./tools/workbench -l plugin.debug -i /src/lib/gsvg.script.js -i /src/plugins/debug.script.js -t test/plugins/debug/cases -p 7004",
    "workbench:plugin.keep.aspect": "node ./tools/workbench -l plugin.keep.aspect -i /src/lib/gsvg.script.js -i /src/plugins/keep.aspect.script.js -t test/plugins/keep.aspect/cases -p 7005",
    "workbench:plugin.observe.resize": "node ./tools/workbench -l plugin.observe.resize -i /src/lib/gsvg.script.js -i /src/plugins/observe.resize.script.js -t test/plugins/observe.resize/cases -p 7006",
    "workbench:plugin.observe.style": "node ./tools/workbench -l plugin.observe.style -i /src/lib/gsvg.script.js -i /src/plugins/observe.style.script.js -t test/plugins/observe.style/cases -p 7007",
    "workbench:plugin.shapes": "node ./tools/workbench -l plugin.shapes -i /src/lib/gsvg.script.js -i /src/plugins/shapes.script.js -t test/plugins/shapes/cases -p 7009",
    "workbench:plugin.template.engine": "node ./tools/workbench -l plugin.template.engine -i /src/lib/gsvg.script.js -i /src/plugins/template.engine.script.js -t test/plugins/template.engine/cases -p 7008",
    "test": "concurrently -m 1 npm:test:*",
    "test:helpers": "playwright test ./test/helpers/ --project=helpers",
    "test:composer": "playwright test test/component/composer/index.test.js --project=composer",
    "test:core": "playwright test test/core/index.test.js --project=core",
    "test:gsvg": "playwright test test/lib/gsvg/index.test.js --project=svg",
    "test:plugin.animateto": "playwright test test/plugins/animateto/index.test.js --project=animateto",
    "test:plugin.debug": "playwright test test/plugins/debug/index.test.js --project=debug",
    "test:plugin.keep.aspect": "playwright test test/plugins/keep.aspect/index.test.js --project=keep.aspect",
    "test:plugin.observe.resize": "playwright test test/plugins/observe.resize/index.test.js --project=observe.resize",
    "test:plugin.observe.style": "playwright test test/plugins/observe.style/index.test.js --project=observe.style",
    "test:plugin.template.engine": "playwright test test/plugins/template.engine/index.test.js --project=template.engine",
    "test:plugin.shapes": "playwright test test/plugins/shapes/index.test.js --project=shapes",
    "build": "concurrently -m 1 npm:build:*",
    "build:core": "node ./tools/packer/index.js -m ./src/core/index.js ./dist/core/index.js ./package.json",
    "build:composer": "node ./tools/packer/index.js src/component/composer.js ./dist/component/composer.js ./package.json",
    "build:gsvg": "node ./tools/packer/index.js -m ./src/lib/gsvg.js ./dist/lib/gsvg.js ./package.json",
    "build:plugin.animateto": "node ./tools/packer/index.js -m src/plugins/animateto.js ./dist/lib/plugins/gsvg.animateto.js",
    "build:plugin.debug": "node ./tools/packer/index.js -m src/plugins/debug.js ./dist/lib/plugins/gsvg.debug.js",
    "build:plugin.keep.aspect": "node ./tools/packer/index.js -m src/plugins/keep.aspect.js ./dist/lib/plugins/gsvg.keep.aspect.js",
    "build:plugin.observe.resize": "node ./tools/packer/index.js -m src/plugins/observe.resize.js ./dist/lib/plugins/gsvg.observe.resize.js",
    "build:plugin.observe.style": "node ./tools/packer/index.js -m src/plugins/observe.style.js ./dist/lib/plugins/gsvg.observe.style.js",
    "build:plugin.template.engine": "node ./tools/packer/index.js -m src/plugins/template.engine.js ./dist/lib/plugins/gsvg.template.engine.js",
    "build:plugin.shapes": "node ./tools/packer/index.js -m src/plugins/shapes.js ./dist/lib/plugins/gsvg.shapes.js",
    "build:plugin.script": "node ./tools/packer/index.js ./src/lib/gsvg.script.js ./dist/script/gsvg.js ./package.json",
    "build:plugin.animateto.script": "node ./tools/packer/index.js -m src/plugins/animateto.js ./dist/script/plugins/gsvg.animateto.js",
    "build:plugin.debug.script": "node ./tools/packer/index.js -m src/plugins/debug.js ./dist/script/plugins/gsvg.debug.js",
    "build:plugin.keep.aspect.script": "node ./tools/packer/index.js -m src/plugins/keep.aspect.js ./dist/script/plugins/gsvg.keep.aspect.js",
    "build:plugin.observe.resize.script": "node ./tools/packer/index.js -m src/plugins/observe.resize.js ./dist/script/plugins/gsvg.observe.resize.js",
    "build:plugin.observe.style.script": "node ./tools/packer/index.js -m src/plugins/observe.style.js ./dist/script/plugins/gsvg.observe.style.js",
    "build:plugin.template.engine.script": "node ./tools/packer/index.js -m src/plugins/template.engine.js ./dist/script/plugins/gsvg.template.engine.js",
    "build:plugin.shapes.script": "node ./tools/packer/index.js -m src/plugins/shapes.js ./dist/script/plugins/gsvg.shapes.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "commander": "^11.1.0",
    "concurrently": "^8.2.2",
    "open": "^9.1.0",
    "playwright": "^1.39.0",
    "ws": "^8.14.2",
    "esbuild": "^0.19.5",
    "esprima-next": "^5.8.4"
  },
  "type": "module"
}
